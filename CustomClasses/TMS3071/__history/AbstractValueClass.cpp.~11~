//---------------------------------------------------------------------------
#include "AbstractValueClass.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
//---------------------------------------------------------------------------
//Конструктор
TAbstractValue::TAbstractValue()
{
	RequestAddress = _RequestAddress;
	AbstractValue = "";
}
//---------------------------------------------------------------------------
//Деструктор
TAbstractValue::~TAbstractValue()
{

}
//---------------------------------------------------------------------------
String TAbstractValue::Read()
{
	return AbstractValue;
}
//---------------------------------------------------------------------------
void TAbstractValue::Update(String Value)
{
    AbstractValue = Value;
}
